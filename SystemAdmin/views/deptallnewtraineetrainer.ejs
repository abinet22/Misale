<body>

  <!-- ======= Header ======= -->
  <%- include ('./partials/headerdept') %>
  <!-- ======= Sidebar ======= -->
 <%- include ('./partials/sidedept') %>
  <main id="main" class="main">

    <div class="pagetitle">
      <h1>TOT(Trainee) List</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/misaleacadamy/dashboard">Home</a></li>
          <li class="breadcrumb-item">Manage TOT(Trainee) Data</li>
          <li class="breadcrumb-item active">New TOT(Trainee) List</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-12">
           <%- include('./partials/messages') %>
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">TOT(Trainee) List</h5>
           
              <!-- Table with stripped rows -->
              <table class="table datatable table-responsive">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Code</th>
                   
                    <th scope="col">Gender/Age</th>

                    <th scope="col">Education Category</th>
                  
                    <th scope="col">Intrance Fee Payed? </th>
                    <th scope="col">Trainee Fee Payed</th>
                    <th scope="col">Certificate Fee Payed</th>
                    <th scope="col">Is Selected</th>
                    <th scope="col">Registration Date</th>
                   

                    <th scope="col">Action</th>
                   
                  
                    
                  </tr>
                </thead>
                <tbody>
                  <% if(trainee.length > 0) { %>
                    <% var i =0 %>
                    <% trainee.forEach(function(row){ %>
                        <% i++ %>
                        <tr>
                          <th scope="row"><%= i %></th>
                          <td><%= row.fullname %></td>
                          <td><span style="font-weight: 600; font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;"><%= row.trainee_code %></span></td>
                         
                         
                         
                          <td><%= row.gender %>/<%= row.age %></td>
                        
                          <% var edu = row.licence_type %>
                          <td> <% if(config.length > 0) { %>
                          
                            <% config.forEach(function(row){ %>
                              <%if(row.config_id==edu){ %>
                                <%= row.config_name %>
                              <% }} )%>
           
                              <%} %></td>
                        
                          <td><%= row.recept_no_intrance===null?"No":"Yes" %></td>
                          <td><%= row.recept_no_training===null?"No":"Yes" %></td>
                          <td><%= row.recept_no_certificate===null?"No":"Yes" %></td>
                          <td><%= row.is_selected %></td>
                          <td><%= new Date(row.createdAt).toLocaleDateString()  %></td>
                          <td><% if(row.is_selected==='No'){ %>
                            <form action="/misaleacadamy/dept/selectnewtotapplicant/<%= row.uniqueid %>" method="post">
                              <button class="btn btn-sm btn-outline-info" type="submit" >Select Applicant </button>
                     
                            </form>
                           <% } else{%>
                          <a class="btn btn-sm btn-outline-success">Already Selected</a>
                      
                         <%}%></td>
                         </tr>
                   <% } )%>
           
              <%} %>
                </tbody>
              </table>
              <!-- End Table with stripped rows -->

            </div>
          </div>

        </div>
      </div>
    </section>

  </main><!-- End #main -->

  <%- include ('./partials/footeradmin') %>
  <div class="modal fade" id="addBookDialog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
           <h4 class="modal-title">Enter Reciept Number To Add Payment</h4>
            <!-- <button type="button" class="close" data-dismiss="modal">X</button> -->
        </div>
   <form action="/misaleacadamy/dept/activatetottraineeaccount" method="post">
    <div class="modal-body">
      <small class="">Payement Type.</small>
      <select class="form-control form-control-sm mt-2" name="paymenttype" >
        <option value="0">Select Payment Type First</option>
        <option>Training</option>
        <option>Certificate</option>
      </select>
      <small class="mt-2 mb-5"> AddReciept No Here.</small>
      <input type="hidden" class="js-toggler__input" value="" name="traineeId" id="traineeId" >
      <input class="form-control form-control-sm" name="receipt_no" type="text">
    
        </div>
         <div class="modal-footer">
             <button type="submit" class="btn btn-sm btn-outline-primary">Save</button>
        </div>
   </form>
    </div>
    </div><!-- /.modal-dialog -->
  </div><!-- /.modal -->
 

  <script>
    $(document).on("click", ".open-AddBookDialog", function (e) {

e.preventDefault();

var _self = $(this);

var myBookId = _self.data('id');
$("#traineeId").val(myBookId);

$(_self.attr('href')).modal('show');
});
  </script>
