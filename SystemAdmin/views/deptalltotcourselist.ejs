<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title></title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="../../../../assets/img/favicon.png" rel="icon">
  <link href="../../../../assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.gstatic.com" rel="preconnect">
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="../../../../assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="../../../../assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="../../../../assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="../../../../assets/vendor/quill/quill.snow.css" rel="stylesheet">
  <link href="../../../../assets/vendor/quill/quill.bubble.css" rel="stylesheet">
  <link href="../../../../assets/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="../../../../assets/vendor/simple-datatables/style.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="../../../../assets/css/style.css" rel="stylesheet">
  <style>
    .tabs {
      display: flex;
    }
    
    .tablinks {
      background-color: #ddd;
      border: none;
      outline: none;
      cursor: pointer;
      padding: 10px;
      margin-right: 10px;
    }
    
    .tablinks.active {
      background-color: #ccc;
    }
    
    .tabcontent {
      display: none;
    }
    
    .course {
      margin-bottom: 10px;
    }
    </style>
    

</head>

<body>

  <!-- ======= Header ======= -->
  <%- include ('./partials/headerdept') %>
  <!-- ======= Sidebar ======= -->
 <%- include ('./partials/sidedept') %>
  <main id="main" class="main">

    <div class="pagetitle">
      <h1>All TOT Course List</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/misaleacadamy/dashboard">Home</a></li>
          <li class="breadcrumb-item">TOT(TRAINEE) SETTING</li>
          <li class="breadcrumb-item">Manage TOT Courses</li>
          <li class="breadcrumb-item active">All TOT Course List</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-12">

          <div class="card">
            <div class="card-body">
              <h5 class="card-title">All TOT Course List</h5>
              <%- include ('./partials/messages') %>
              

              <div class="">
              
                <!-- Bordered Tabs Justified -->
                <div class="tabs">
                    <% course.forEach(function(type) { %>
                      <button class="tablinks nav-link w-100" onclick="openTab(event, '<%= type.config_id %>')"><%= type.config_name %></button>
                    <% }); %>
                  </div>
                  <% course.forEach(function(type) { %>
                    <div id="<%= type.config_id %>" class="tabcontent">
                    
                        <% var courseid = type.config_id %>
                        <table class="table datatable table-responsive" id="myTable">
                            <thead>
                              <tr>
                                <th scope="col">#</th>
                                <th scope="col">Course Name</th>
                                <th scope="col">Course Code</th>
                               
                                <th scope="col">Course Weight</th>
            
                                <th scope="col">Education Category</th>
                                <th scope="col">Course Type</th> 
                                <th scope="col">Course Part</th> 
                                <th scope="col">Action</th> 
                              </tr>
                            </thead>
                            <tbody>
                              <% if(totcourse.length > 0) { %>
                                <% var i =0 %>
                                <% totcourse.forEach(function(row){ %>
                                    <% i++ %>
                                   <% if(courseid === row.config_id){ %>
                                    
                                    <tr>
                                      <th scope="row"><%= i %></th>
                                      <td><%= row.course_name %></td>
                                      <td><span style="font-weight: 600; font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;"><%= row.course_code %></span></td>
                                     
                                      <td><%= row.course_weight %></td>
                                     
                                      <td>  <%= row.config_name %></td>
                                    
                                    
                                     
                                     
                                      <td><%= row.course_type %></td>
                                      <td><%= row.course_part %></td>
                                    
                                     <td >
            
                                        <form action="/misaleacadamy/dept/deletetotcourse/<%= row.c_id %>" method="post"> 
                                         
                                              <button class=" btn btn-sm btn-block btn-outline-danger" type="submit">Delete</button>
                                           </form>
                                     
                                     </td>
                                      </tr>
                               <%} } )%>
                       
                          <%} %>
                            </tbody>
                          </table>
                     
                    </div>
                  <% }); %>
              
              </div>
              <!-- Table with stripped rows -->
             
              <!-- End Table with stripped rows -->

            </div>
          </div>

        </div>
      </div>
    </section>

  </main><!-- End #main -->

  <%- include ('./partials/footeradmin') %>

  <!-- Vendor JS Files -->
  <script src="../../../../assets/vendor/apexcharts/apexcharts.min.js"></script>
  <script src="../../../../assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="../../../../assets/vendor/chart.js/chart.umd.js"></script>
  <script src="../../../../assets/vendor/echarts/echarts.min.js"></script>
  <script src="../../../../assets/vendor/quill/quill.min.js"></script>
  <script src="../../../../assets/vendor/simple-datatables/simple-datatables.js"></script>
  <script src="../../../../assets/vendor/tinymce/tinymce.min.js"></script>
  <script src="../../../../assets/vendor/php-email-form/validate.js"></script>

  <!-- Template Main JS File -->
  <script src="../../../../assets/js/main.js"></script>
 
    <script>
    function openTab(evt, tabName) {
      var i, tabcontent, tablinks;
      tabcontent = document.getElementsByClassName("tabcontent");
      for (i = 0; i < tabcontent.length; i++) {
        tabcontent[i].style.display = "none";
      }
      tablinks = document.getElementsByClassName("tablinks");
      for (i = 0; i < tablinks.length; i++) {
        tablinks[i].classList.remove("active");
      }
      document.getElementById(tabName).style.display = "block";
      evt.currentTarget.classList.add("active");
    }
    </script>
</body>

</html>