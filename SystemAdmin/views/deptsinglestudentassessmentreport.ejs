<style>
    table {
  border-collapse: collapse;
}

td {
  padding: 8px;
  position: relative; /* set position to relative to enable absolute positioning of pseudo-element */
}

td:before {
  content: "";
  position: absolute;
  top: 0;
  left: 50%;
  width: 1px;
  height: 100%;
  background-color: black;
  transform: translateX(-50%);
  z-index: -1; /* set z-index to -1 to position behind the content */
}

tr:first-child td:before {
  /* don't draw line for first row */
  display: none;
}

</style>
<body>

  <!-- ======= Header ======= -->
  <%- include ('./partials/headerdept') %>
  <!-- ======= Sidebar ======= -->
 <%- include ('./partials/sidedept') %>
  <main id="main" class="main">

    <div class="pagetitle">
      <h1>Candidate(Tehadso) Data</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/dashboard">Home</a></li>
          <li class="breadcrumb-item">Manage Candidate(Tehadso) Data</li>
          <li class="breadcrumb-item active">Single Candidate(Tehadso) Data</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-12">
           
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Candidate(Tehadso)  Assessement Data</h5>
              <div class="">
             
                <!-- Bordered Tabs Justified -->
                <ul class="nav nav-tabs nav-tabs-bordered d-flex" id="borderedTabJustified" role="tablist">
                  <li class="nav-item flex-fill" role="presentation">
                    <button class="nav-link w-100 active" id="home-tab" data-bs-toggle="tab" data-bs-target="#bordered-justified-home" type="button" role="tab" aria-controls="home" aria-selected="true">Theoretical</button>
                  </li>
                  <li class="nav-item flex-fill" role="presentation">
                    <button class="nav-link w-100" id="profile-tab" data-bs-toggle="tab" data-bs-target="#bordered-justified-profile" type="button" role="tab" aria-controls="profile" aria-selected="false" tabindex="-1">Practical</button>
                  </li>
                
                </ul>
                <div class="tab-content pt-2" id="borderedTabJustifiedContent">
                  <div class="tab-pane fade show active" id="bordered-justified-home" role="tabpanel" aria-labelledby="home-tab">
                    <table class="table datatable table-responsive">
                        <thead>
                          <tr>
                            <th scope="col"></th>
                            <th scope="col"></th>
                            <th scope="col">Date</th>
                            <th scope="col">Name</th>
                            <th scope="col">Code</th>
                           
                            <th scope="col">Gender/Age</th>
        
                            <th scope="col">Education Category</th>
                            <th scope="col">School</th>
                            <th scope="col">No Of Attempt</th>
                           
                            <th scope="col">Total Score</th>
                            
                        
                            <th scope="col">Result Score</th>
                         
                          </tr>
                        </thead>
                        <tbody>
                            <% const statuses = ['text-success', 'text-error', 'text-danger', 'text-warning']; %>
        
                          <% if(tresult.length > 0) { %>
                            <% var i =0 %>
                            <% tresult.forEach(function(row){ %>
                                <% i++ %>
                                <% const statusIndex = i % statuses.length %>
                                <% const statusClass = statuses[statusIndex] %>
                                <tr>
                                    <td><% var min =parseFloat(row.finish_time)  %>
                                        <%= min.toFixed(2) %> Min
                                            </td>
                                  <td scope="row"><i class="bi bi-circle-fill activity-badge <%=statusClass %> align-self-start"></i></td>
                                  <td><%var date= new Date(row.createdAt);
                                    var ethiopianDate = toEthiopianDate(date);
                                    function toEthiopianDate(date) {
                                        const year = date.getFullYear();
                                        const month = date.getMonth() + 1;
                                        const day = date.getDate();
                                      
                                        // Ethiopian calendar starts on September 11 (or 12 on leap years)
                                        const ethiopianYear = year - 8;
                                        const ethiopianMonth = (month + 3) % 12 || 12;
                                        const ethiopianDay = day + 17 - (month > 9 ? 1 : 0) - (ethiopianMonth === 13 ? 1 : 0);
                                      
                                        return `${ethiopianMonth.toString().padStart(2, '0')}/${ethiopianDay.toString().padStart(2, '0')}/${ethiopianYear.toString()}`;
                                      }
                                      
                                    %>
                                    <%= date %>G.C
                                  
                                    <span style="font-weight: 600; font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;">  <%= ethiopianDate %>E.C</span>
                                </td>
                                  <% if(trainee) { %>
                          
                                  
                                    <td><%= trainee.fullname %></td>
                                    <td><span style="font-weight: 600; font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;"><%= trainee.trainee_code %></span></td>
                                   
                                    <td><%= trainee.gender %>/<%= trainee.age %></td>
                                    <% var school = trainee.school_from %>
                                    <% var edu = trainee.licence_type %>
                                    <td> <% if(config.length > 0) { %>
                                    
                                      <% config.forEach(function(row){ %>
                                        <%if(row.config_id==edu){ %>
                                          <%= row.config_name %>
                                        <% }} )%>
                     
                                        <%} %></td>
                                    <td>   <% if(config.length > 0) { %>
                                    
                                      <% config.forEach(function(row){ %>
                                        <%if(row.config_id==school){ %>
                                          <%= row.config_name %>
                                        <% }} )%>
                     
                                        <%} %>
                                      
                                      
                                      </td>
                      
                     
                        <%} %>
                        <td>  <%=row.attempt_no_theory %></td>
                                
                                  <td>100</td>
                                
                               
                                  <td>

                                    <% if( row.theoretical_score >= 74) { %>
                                      <span style="color: green;" ><%= row.theoretical_score %></span>
                                      <% }else { %>
                                        <span style="color: red;" ><%= row.theoretical_score %></span>
                                        <% }%>
                                  </td>
                               
                              
                                  </tr>
                           <% } )%>
                   
                      <%} %>
                        </tbody>
                      </table>
                </div>
                  <div class="tab-pane fade" id="bordered-justified-profile" role="tabpanel" aria-labelledby="profile-tab">
                    <table class="table datatable table-responsive">
                        <thead>
                          <tr>
                            <th scope="col"></th>
                            <th scope="col"></th>
                            <th scope="col">Date</th>
                            <th scope="col">Name</th>
                            <th scope="col">Code</th>
                           
                            <th scope="col">Gender/Age</th>
        
                            <th scope="col">Education Category</th>
                            <th scope="col">School</th>
                            <th scope="col">No Of Attempt</th>
                            <th scope="col">Assessment Technical Part</th>
                            <th scope="col">Total Score</th>
                            
                            <th scope="col">Assessement Detail</th>
                            <th scope="col">Result Score</th>
                            <th scope="col">Assess By</th>
                            
                          </tr>
                        </thead>
                        <tbody>
                           
                          <% if(presults.length > 0) { %>
                            <% var i =0 %>
                            <% presults.forEach(function(row){ %>
                                <% i++ %>
                                <% const statusIndex = i % statuses.length %>
                                <% const statusClass = statuses[statusIndex] %>
                                <tr>
                                    <td><% var min =parseFloat(row.finish_time)  %>
                                        <%= min.toFixed(2) %> Min
                                            </td>
                                  <td scope="row"><i class="bi bi-circle-fill activity-badge <%=statusClass %> align-self-start"></i></td>
                                  <td><%var date= new Date(row.createdAt);
                                    var ethiopianDate = toEthiopianDate(date);
                                    function toEthiopianDate(date) {
                                        const year = date.getFullYear();
                                        const month = date.getMonth() + 1;
                                        const day = date.getDate();
                                      
                                        // Ethiopian calendar starts on September 11 (or 12 on leap years)
                                        const ethiopianYear = year - 8;
                                        const ethiopianMonth = (month + 3) % 12 || 12;
                                        const ethiopianDay = day + 17 - (month > 9 ? 1 : 0) - (ethiopianMonth === 13 ? 1 : 0);
                                      
                                        return `${ethiopianMonth.toString().padStart(2, '0')}/${ethiopianDay.toString().padStart(2, '0')}/${ethiopianYear.toString()}`;
                                      }
                                      
                                    %>
                                    <%= date %>G.C
                                  
                                    <span style="font-weight: 600; font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;">  <%= ethiopianDate %>E.C</span>
                                </td>
                                  <% if(trainee) { %>
                          
                                  
                                    <td><%= trainee.fullname %></td>
                                    <td><span style="font-weight: 600; font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;"><%= trainee.trainee_code %></span></td>
                                   
                                    <td><%= trainee.gender %>/<%= trainee.age %></td>
                                    <% var school = trainee.school_from %>
                                    <% var edu = trainee.licence_type %>
                                    <td> <% if(config.length > 0) { %>
                                    
                                      <% config.forEach(function(row){ %>
                                        <%if(row.config_id==edu){ %>
                                          <%= row.config_name %>
                                        <% }} )%>
                     
                                        <%} %></td>
                                    <td>   <% if(config.length > 0) { %>
                                    
                                      <% config.forEach(function(row){ %>
                                        <%if(row.config_id==school){ %>
                                          <%= row.config_name %>
                                        <% }} )%>
                     
                                        <%} %>
                                      
                                      
                                      </td>
                      
                     
                        <%} %>
                        <td>  <%=row.attempt_no %></td>
                                  <td><%= row.assessment_part %></td>
                                  <td><%= row.total_point %></td>
                                
                                
                                  <td><% var data =  row.assessment_detail %>
                                    <% data.forEach(item => { %>
                                        <li><%= item.name %> with point: <%= item.point %></li>
                                      <% }) %>
                                  </td>
                                  <td>
                                    <% if( row.score >= 74) { %>
                                      <span style="color: green;" ><%= row.score %></span>
                                      <% }else { %>
                                        <span style="color: red;" ><%= row.score %></span>
                                        <% }%>
                                  </td>
                                 <td>
                                 <%= row.full_name %>
                                 </td>
                              
                                  </tr>
                           <% } )%>
                   
                      <%} %>
                        </tbody>
                      </table>
                </div>
               
                </div><!-- End Bordered Tabs Justified -->
  
              </div>
            
              <!-- Table with stripped rows -->
             
              <!-- End Table with stripped rows -->

            </div>
          </div>

        </div>
      </div>
    </section>

  </main><!-- End #main -->

  <%- include ('./partials/footeradmin') %>

