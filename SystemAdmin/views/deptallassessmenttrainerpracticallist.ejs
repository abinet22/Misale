<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title></title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="../../assets/img/favicon.png" rel="icon">
  <link href="../../assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.gstatic.com" rel="preconnect">
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="../../assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="../../assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="../../assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="../../assets/vendor/quill/quill.snow.css" rel="stylesheet">
  <link href="../../assets/vendor/quill/quill.bubble.css" rel="stylesheet">
  <link href="../../assets/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="../../assets/vendor/simple-datatables/style.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="../../assets/css/style.css" rel="stylesheet">


</head>

<body>

  <!-- ======= Header ======= -->
  <%- include ('./partials/headerdept') %>
  <!-- ======= Sidebar ======= -->
 <%- include ('./partials/sidedept') %>
  <main id="main" class="main">

    <div class="pagetitle">
      <h1>Practical Assessements List</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/misaleacadamy/dashboard">Home</a></li>
          <li class="breadcrumb-item">TOT(TRAINEE) SETTING</li>
          <li class="breadcrumb-item">Manage TOT Practial Assessements</li>
          <li class="breadcrumb-item active">All Practical Assessements List</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-12">

          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Practical Assessements List</h5>
           
              <!-- Table with stripped rows -->
              <table class="table datatable table-responsive">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Main Checklist</th>
                    <th scope="col">Sub Checklist</th>
                   
                    <th scope="col">Technician Part</th>
                    <th scope="col">Total Point</th>=
                    <th scope="col">Is Active</th>

                   
                    <th scope="col">Action</th>
                    
                  </tr>
                </thead>
                <tbody>
                  <% if(assessment.length > 0) { %>
                    <% var d =0 %>
                    <% assessment.forEach(function(row){ %>
                        <% d++ %>
                        <tr>
                          <th scope="row"><%= d %></th>
                          <td><%= row.main_checklist %></td>
                          <td><span style="font-weight: 600; font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;">
                            <%
                            var selectioncriteria= [];
                           var x =[];
                           var z= [];
                           selectioncriteria=  JSON.parse(JSON.stringify(row.sub_checklist)); %>
                           <% for (let i = 0; i < selectioncriteria.length; i++) { %>
                            <label><%= selectioncriteria[i].id %> : &nbsp;
                            <%= selectioncriteria[i].name %> <!-- label with object name -->
                          ( <%= selectioncriteria[i].point %> Point) </label> <!-- label with object point -->
                            <!-- <input type="number" name="criteria-<%= i %>" value="<%= selectioncriteria[i].point %>"/> <!-- input element -->
                          
                            <% } %>
                          
                        </span></td>
                        <td><%= row.technician_part %></td>
                          <td><%= row.total_point %></td>
                          
                          
                         
                      
                         
                          <td><%= row.is_active %></td>
                      
                          <td>
                            <%var isactive= row.is_active %>
                            <% if(isactive =='Yes') {%>
                                <form method="post" action="/misaleacadamy/dept/diactivateassessment">
                                    <button class=" btn btn-sm btn-block btn-outline-danger" type="submit">DiActivate</button>
                                </form>
                                <%}else{%>
                                    <form method="post" action="/dept/diactivateassessment">
                                        <button class=" btn btn-sm btn-block btn-outline-success" type="submit">Activate</button>
                                    </form>
                                <%}%>
                                <form method="post" action="/misaleacadamy/dept/editassessment">
                                   
                         <button class=" mt-2 btn btn-sm btn-block btn-outline-info" type="submit">Edit</button></td>
                        </form>
                            </tr>
                   <% } )%>
           
              <%} %>
                </tbody>
              </table>
              <!-- End Table with stripped rows -->

            </div>
          </div>

        </div>
      </div>
    </section>

  </main><!-- End #main -->

  <%- include ('./partials/footeradmin') %>

  <!-- Vendor JS Files -->
  <script src="../../assets/vendor/apexcharts/apexcharts.min.js"></script>
  <script src="../../assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="../../assets/vendor/chart.js/chart.umd.js"></script>
  <script src="../../assets/vendor/echarts/echarts.min.js"></script>
  <script src="../../assets/vendor/quill/quill.min.js"></script>
  <script src="../../assets/vendor/simple-datatables/simple-datatables.js"></script>
  <script src="../../assets/vendor/tinymce/tinymce.min.js"></script>
  <script src="../../assets/vendor/php-email-form/validate.js"></script>

  <!-- Template Main JS File -->
  <script src="../../assets/js/main.js"></script>

</body>

</html>