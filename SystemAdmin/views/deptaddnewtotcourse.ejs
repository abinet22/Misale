  <title></title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="../../assets/img/favicon.png" rel="icon">
  <link href="../../assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.gstatic.com" rel="preconnect">
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="../../assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="../../assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="../../assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="../../assets/vendor/quill/quill.snow.css" rel="stylesheet">
  <link href="../../assets/vendor/quill/quill.bubble.css" rel="stylesheet">
  <link href="../../assets/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="../../assets/vendor/simple-datatables/style.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="../../assets/css/style.css" rel="stylesheet">


</head>

<body>

  <%- include ('./partials/headerdept') %>
  <!-- ======= Sidebar ======= -->
 <%- include ('./partials/sidedept') %>
  <main id="main" class="main">

    <div class="pagetitle">
      <h1>Add New TOT Course</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/misaleacadamy/dashboard">Home</a></li>
          <li class="breadcrumb-item">TOT(TRAINEE) SETTING</li>
          <li class="breadcrumb-item">Manage TOT Courses</li>
          <li class="breadcrumb-item active">Add New TOT Course</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->
    <section class="section">
      <div class="row">
        <div class="col-lg-8 offset-md-2">

          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Add New TOT Course Form</h5>
              <%- include ('./partials/messages') %>
              <!-- Horizontal Form -->
              <form method="post" action="/misaleacadamy/dept/addnewtotcourse">
                <div class="row">

                  <div class="col-md-6">
                    <div class="form-group">
                     <label for="inputEmail3" class=" col-form-label">Course Name</label>
               
                     <input type="text" name="coursename" autocomplete="off" class="form-control" id="inputText">
                   </div>
                 </div>
                 
                 <div class="col-md-6">
                    <div class="form-group">
                     <label for="inputEmail3" class="col-form-label">Course Code</label>
               
                     <input type="text" name="coursecode" autocomplete="off" class="form-control" id="inputEmail">
                   </div>
                 </div>
                </div>
              
                <div class="row">
                  <div class="col-md-6">
                    <div class="form-group">
                     <label for="inputEmail3" class=" col-form-label">Course Type</label>
               
                     <select name="coursetype"  class="form-control" >
                       <option value="0">Please Select Course Category</option>
                       <option value="Theoretical">Theoretical</option>
                       <option value="Practical">Practical</option>
                     
                  
                  
                           
                     </select>
                   </div>
               </div>
                  
                <div class="col-md-6">
                  <div class="form-group">
                    <label for="inputEmail3" class="col-form-label">Course Weight</label>
                 
                    <input type="text" name="courseweight" autocomplete="off" class="form-control" id="inputEmail" pattern="[0-9]*[.,]?[0-9]+">
                  </div>
                </div>
                </div>
                <div class="row">
                  <div class="col-md-6">
                    <div class="form-group">
                      <label for="inputEmail3" class=" col-form-label">Education Category</label>
               
                      <select name="courseeducation"  class="form-control" >
                        <option value="0">Please Select Education Category</option>
                        <% if(config.length > 0) { %>
                          <% var i =0 %>
                          <% config.forEach(function(row){ %>
                          <option value="<%= row.config_id %>"><%= row.config_name %></option>
                         <%  } )}%>
                 
                   
                            
                      </select>
                    </div>
                  </div>
                  <div class="col-md-6">
                   
                    <div class="form-group">
                      <label for="inputEmail3" class=" col-form-label">Course Part</label>
                      <select name="coursepart"  class="form-control" >
                        <option value="0">Please Select Course Part</option>
                        <option value="Continuous_1">Continues Part 1</option>
                        <option value="Continuous_2">Continues Part 2</option>
                        <option value="Continuous_3">Continues Part 3</option>
                        <option value="Continuous_4">Continues Part 4</option>
                        <option value="Final_1">Final Part 1</option>
                        <option value="Final_2">Final Part 2</option>
                        <option value="Final_3">Final Part 3</option>
                        <option value="Final_4">Final Part 4</option>
                        <option value="Other">Other</option>
                      
                   
                   
                            
                      </select>
                    </div>
                </div>
                  
                </div>
               
               
                <div class="text-center">
                  <button type="submit" class="btn btn-sm btn-outline-primary">Submit</button>
                 
                </div>
              </form><!-- End Horizontal Form -->

            </div>
          </div>

        

        </div>

        
      </div>
    </section>

  </main><!-- End #main -->

  <!-- ======= Footer ======= -->
 <%- include('./partials/footeradmin') %>

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

