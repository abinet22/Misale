</head>

<body>

  <!-- ======= Header ======= -->
  <%- include ('./partials/headeradmin') %>
  <!-- ======= Sidebar ======= -->
 <%- include ('./partials/sideadmin') %>
  <main id="main" class="main">

    <div class="pagetitle">
      <h1>Candidate(Tehadso) List</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/misaleacadamy/dashboard">Home</a></li>
          <li class="breadcrumb-item">Manage Candidate(Tehadso)</li>
          <li class="breadcrumb-item active">Update Candidate(Tehadso) Payment</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-12">
           <%- include('./partials/messages') %>
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Update Candidate(Tehadso) Payment</h5>
           
              <!-- Table with stripped rows -->
              <table class="table datatable ">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Code</th>
                   
                    <th scope="col">Gender/Age</th>

                    <th scope="col">Education Category</th>
                    <th scope="col">School</th>
                    <th scope="col">Is Active</th>
                    <th scope="col">Is Payed</th>
                    <th scope="col">Reciept No</th>
                    <th scope="col">No Of Attempt</th>
                    <th scope="col">Action</th>
                    
                  </tr>
                </thead>
                <tbody>
                  <% if(trainee.length > 0) { %>
                    <% var i =0 %>
                    <% trainee.forEach(function(row){ %>
                        <% i++ %>
                        <tr>
                          <th scope="row"><%= i %></th>
                          <td><%= row.fullname %></td>
                          <td><span style="font-weight: 600; font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;"><%= row.trainee_code %></span></td>
                         
                         
                         
                          <td><%= row.gender %>/<%= row.age %></td>
                          <% var school = row.school_from %>
                          <% var edu = row.school_from %>
                          <td> <% if(config.length > 0) { %>
                          
                            <% config.forEach(function(row){ %>
                              <%if(row.config_id==edu){ %>
                                <%= row.config_name %>
                              <% }} )%>
           
                              <%} %></td>
                          <td>   <% if(config.length > 0) { %>
                          
                            <% config.forEach(function(row){ %>
                              <%if(row.config_id==school){ %>
                                <%= row.config_name %>
                              <% }} )%>
           
                              <%} %>
                            
                            
                            </td>
                          <td><%= row.is_active %></td>
                          <td><%= row.is_payed %></td>
                          <td><%= row.recept_no %></td>
                          
                          <td><%= row.attempt_count %></td>
                         <td>
                         <% if(row.is_payed ==='Yes'){ %>
                          <a title="Add this item" class="open-AddBookDialog btn btn-sm btn-block btn-outline-info">Already Activated</a>
                       
                        <%  }else{ %>
                            <a data-id="<%= row.uniqueid %>" title="Add this item" class="open-AddBookDialog btn btn-sm btn-block btn-outline-success" href="#addBookDialog">Activate</a>
                         <% } %>
                         </td>
                          </tr>
                   <% } )%>
           
              <%} else{ %>
                <a title="Add this item" class="open-AddBookDialog btn btn-sm btn-block btn-outline-info">All Candidates Are Payed And Activated</a>
                      
               <% } %>
                </tbody>
              </table>
              <!-- End Table with stripped rows -->

            </div>
          </div>

        </div>
      </div>
    </section>

  </main><!-- End #main -->

  <%- include ('./partials/footeradmin') %>
  <div class="modal fade" id="addBookDialog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
           <h4 class="modal-title">Enter Reciept Number To Activate</h4>
            <!-- <button type="button" class="close" data-dismiss="modal">X</button> -->
        </div>
   <form action="/misaleacadamy/office/activatetraineeaccount" method="post">
    <div class="modal-body">
      <label>Please Add Reciept No Here</label>
      <input type="hidden" class="js-toggler__input" value="" name="traineeId" id="traineeId" >
      <input class="form-control mb-2" name="receipt_no" placeholder="Reciept No">
      <input class="form-control mb-2" name="age" type="number" placeholder="Age">
      <select class="form-control mb-2" name="gender">
        <option value="0">Please Select Gender</option>
        <option value="Male">Male</option>
        <option value="Female">Female</option>
      </select>
        </div>
         <div class="modal-footer">
             <button type="submit" class="btn btn-sm btn-outline-primary">Save</button>
        </div>
   </form>
    </div>
    </div><!-- /.modal-dialog -->
  </div><!-- /.modal -->
 

  <script>
    $(document).on("click", ".open-AddBookDialog", function (e) {

e.preventDefault();

var _self = $(this);

var myBookId = _self.data('id');
$("#traineeId").val(myBookId);

$(_self.attr('href')).modal('show');
});
  </script>
  <!-- Vendor JS Files -->
  <script src="../../assets/vendor/apexcharts/apexcharts.min.js"></script>
  <script src="../../assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="../../assets/vendor/chart.js/chart.umd.js"></script>
  <script src="../../assets/vendor/echarts/echarts.min.js"></script>
  <script src="../../assets/vendor/quill/quill.min.js"></script>
  <script src="../../assets/vendor/simple-datatables/simple-datatables.js"></script>
  <script src="../../assets/vendor/tinymce/tinymce.min.js"></script>
  <script src="../../assets/vendor/php-email-form/validate.js"></script>

  <!-- Template Main JS File -->
  <script src="../../assets/js/main.js"></script>

</body>

</html>